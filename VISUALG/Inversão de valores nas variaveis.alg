algoritmo "ex6"

var
 a, b, troca: real
 
 inicio
 
 escreval ("Digite o valor de A")
 leia (a)
 escreval ("Digite o valor de B")
 leia (b)
 
 troca <- a
  a <- b
  b <- troca
 escreval (" ")
 escreval ("Após a troca, o valor de A é: ")
 escreval (a)
 escreval ("E o valor de B é: ")
 escreval (b)
 
 fimalgoritmo
