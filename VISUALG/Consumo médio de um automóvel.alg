algoritmo "ex3"

var

cM, dT, cG: real

inicio

escreval ("Digite a distancia total percorrida pelo automovel. ")
leia  (dT)
escreval ("Digite o total de combustivel gasto.")
leia (cG)
escreval (" ")

cM <- dT / cG

escreva ("O consumo médio do automovel foi de", cM)
escreva  (" km por litro")


FIMALGORITMO
