algoritmo "ex8"

var

rs, us, conv: real

inicio

escreval ("o valor em real é: ")
leia (rs)

escreval ("o valor do dolar na conversão: ")
leia (conv)

us <- rs * conv

escreva ("na conversao")
escreva (rs)
escreval (" reais se tornou ")
escreva (us)
escreva (" dolares")
fimalgoritmo
